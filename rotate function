class Solution:
    def maxRotateFunction(self, nums: List[int]) -> int:
        n = len(nums)
        tot = sum(nums)
        curr = sum(i* nums[i] for i in range(n))
        max_val = curr
        for k in range(1,n):
            curr = curr + tot - n * nums[n-k]
            max_val = max(max_val,curr)
        return max_val
